package com.dds.dom.xml.test;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.InputStream;
import java.util.logging.Logger;

import javax.xml.parsers.DocumentBuilder;
import javax.xml.parsers.DocumentBuilderFactory;

import org.w3c.dom.Document;
import org.w3c.dom.Element;
import org.w3c.dom.NodeList;

public class WfinitParser 
{
	protected static Logger logger = Logger.getLogger("WfinitParser.class");
	public static void main(String[] args) 
	{
		try
		{
			InputStream inStream = new FileInputStream( "E:/data/domparsingData/wfinit-config.xml" );
			DocumentBuilderFactory documentBuildFact = DocumentBuilderFactory.newInstance();
			DocumentBuilder docBuilder = documentBuildFact.newDocumentBuilder();
			Document doc = docBuilder.parse( inStream );
			NodeList workFlowNodeList = doc.getElementsByTagName("workflow");
		}
		catch( FileNotFoundException fnfe )
		{
			logger.info("@@@@@@@@ wfinit-config.xml file was not found in the apprpriate location @@@@@@@@@@@@");
			fnfe.printStackTrace();
		}
		catch( Exception e )
		{
			logger.info("Error found in try block, plz debug it.......");
			e.printStackTrace();
		}
		
	} 
	

}
