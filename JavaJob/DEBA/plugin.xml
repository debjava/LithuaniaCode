<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="DEBA"
   name="DEBA Plug-in"
   version="1.0.0"
   provider-name=""
   class="deba.Activator">

   <runtime>
      <library name="DEBA.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
   </requires>

   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            nameFilter="plugin.xml"
            id="DEBA.contribution1">
         <menu
               label="New Submenu"
               path="additions"
               id="DEBA.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               label="New Action"
               class="deba.popup.actions.NewAction"
               menubarPath="DEBA.menu1/group1"
               enablesFor="multiple"
               id="DEBA.newAction">
         </action>
      </objectContribution>
   </extension>

</plugin>
